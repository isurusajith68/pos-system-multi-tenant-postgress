CREATE TABLE IF NOT EXISTS &quot;custom_products&quot; (
    &quot;custom_product_id&quot; TEXT NOT NULL PRIMARY KEY,
    &quot;name&quot; TEXT NOT NULL,
    &quot;price&quot; REAL NOT NULL,
    &quot;created_at&quot; DATETIME NOT NULL DEFAULT CURRENT_TIMESTAMP,
    &quot;updated_at&quot; DATETIME NOT NULL
);


-- CREATE TABLE &quot;sales_details_backup&quot; AS SELECT * FROM &quot;sales_details&quot;;


-- Create new sales_details table with updated schema
CREATE TABLE &quot;sales_details_new&quot; (
    &quot;sales_detail_id&quot; TEXT NOT NULL PRIMARY KEY,
    &quot;invoice_id&quot; TEXT NOT NULL,
    &quot;product_id&quot; TEXT,
    &quot;custom_product_id&quot; TEXT,
    &quot;unit&quot; TEXT NOT NULL DEFAULT 'pcs',
    &quot;originalPrice&quot; REAL NOT NULL DEFAULT 0,
    &quot;cost_price&quot; REAL NOT NULL DEFAULT 0,
    &quot;quantity&quot; REAL NOT NULL,
    &quot;unit_price&quot; REAL NOT NULL,
    &quot;tax_rate&quot; REAL NOT NULL DEFAULT 0,
    &quot;created_at&quot; DATETIME NOT NULL DEFAULT CURRENT_TIMESTAMP,
    &quot;updated_at&quot; DATETIME NOT NULL,
    CONSTRAINT &quot;sales_details_product_id_fkey&quot; FOREIGN KEY (&quot;product_id&quot;) REFERENCES &quot;products&quot; (&quot;product_id&quot;) ON DELETE SET NULL ON UPDATE CASCADE,
    CONSTRAINT &quot;sales_details_custom_product_id_fkey&quot; FOREIGN KEY (&quot;custom_product_id&quot;) REFERENCES &quot;custom_products&quot; (&quot;custom_product_id&quot;) ON DELETE SET NULL ON UPDATE CASCADE,
    CONSTRAINT &quot;sales_details_invoice_id_fkey&quot; FOREIGN KEY (&quot;invoice_id&quot;) REFERENCES &quot;sales_invoices&quot; (&quot;invoice_id&quot;) ON DELETE RESTRICT ON UPDATE CASCADE
);

-- Copy data from old table to new table
INSERT INTO &quot;sales_details_new&quot; (
    &quot;sales_detail_id&quot;,
    &quot;invoice_id&quot;,
    &quot;product_id&quot;,
    &quot;custom_product_id&quot;,
    &quot;unit&quot;,
    &quot;originalPrice&quot;,
    &quot;cost_price&quot;,
    &quot;quantity&quot;,
    &quot;unit_price&quot;,
    &quot;tax_rate&quot;,
    &quot;created_at&quot;,
    &quot;updated_at&quot;
)
SELECT 
    &quot;sales_detail_id&quot;,
    &quot;invoice_id&quot;,
    &quot;product_id&quot;,
    NULL as &quot;custom_product_id&quot;,
    &quot;unit&quot;,
    &quot;originalPrice&quot;,
    &quot;cost_price&quot;,
    &quot;quantity&quot;,
    &quot;unit_price&quot;,
    &quot;tax_rate&quot;,
    &quot;created_at&quot;,
    &quot;updated_at&quot;
FROM &quot;sales_details&quot;;

-- Drop old table and rename new one
DROP TABLE &quot;sales_details&quot;;
ALTER TABLE &quot;sales_details_new&quot; RENAME TO &quot;sales_details&quot;;</sql><current_tab id="0"/></tab_sql></sqlb_project>
